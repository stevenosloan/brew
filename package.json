    "file-loader": "^0.9.0",
    "gh-pages": "^1.0.0",
    "html-webpack-harddisk-plugin": "^0.1.0",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.5.0",
    "offline-plugin": "^4.8.3",
    "postcss-loader": "^2.0.6",
    "sass-loader": "^5.0.1",
    "style-loader": "^0.18.2",
    "vue-loader": "^12.1.0",
